{
  "IdentitySettings": {
    // FoxIDs endpoint "https://foxidsxxxx.azurewebsites.net" or custom domain "https://foxidsxxxx.com" or local development https://localhost:44330
    "Authority": "https://foxidsxxxx.azurewebsites.net/{tenant_name}/master/foxids_control_client(login)/",
    "ClientId": "foxids_control_client",
    "Scope": "email foxids_control_api:foxids_tenant",
    "MasterScope": "foxids_control_api:foxids_master",
    "LoginCallBackPath": "{tenant_name}/authentication/login_callback",
    "LogoutCallBackPath": "{tenant_name}/authentication/logout_callback"
  },
  "AppSettings": {
    "FoxIDsApiSampleMethod": "values"
  }
}
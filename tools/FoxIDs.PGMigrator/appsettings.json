{
  "Logging": {
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss:fffzzz ",
        "UseUtcTimestamp": true
      }
    },
    "LogLevel": {
      "Default": "Information",
      "System": "Error",
      "Microsoft": "Error"
    }
  },
  "Settings": {
    "PostgreSql": {
      "ConnectionString": "Host=localhost;Username=postgres;Password=postgres;Database=foxids"
    },
    "CosmosDb": {
      "EndpointUri": "xxx",
      "PrimaryKey": "xxx"
    }
  }
}